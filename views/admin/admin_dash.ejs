        <meta name="twitter:card" content="summary_large_image" />
        <meta
        name="linkedin:card"
        content="https://schemester.web.app/graphic/icons/schemester512.png"
        />
        <link
        rel="icon"
        href="/graphic/icons/schemester128.png"
        type="image/png"
        sizes="128x128"
        />
        <link rel="apple-touch-icon" href="/graphic/icons/schemester128.png" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="#216bf3" />
        <meta name="apple-mobile-web-app-title" content="Schemester" />
